@article{ ISI:000374616600006,
Author = {Kang, Sung-Min and Jang, Sung-Chan and Huh, Yun Suk and Lee, Chang-Soo
   and Roh, Changhyun},
Title = {{A highly facile and selective Chemo-Paper-Sensor (CPS) for detection of
   strontium}},
Journal = {{CHEMOSPHERE}},
Year = {{2016}},
Volume = {{152}},
Pages = {{39-46}},
Month = {{JUN}},
Abstract = {{Chemosensors have attracted increasing attention for their usefulness
   on-site detection and monitoring. In this study, we elucidated a novel,
   facile, and highly selective Chemo-Paper-Sensor (CPS) for detection and
   monitoring of strontium (Sr2+) ions, which means a potent colorimetric
   sensor based on a Chrysoidine G (CG)-coated paper strip. The CPS for
   highly selective colorimetric detection of strontium ion was handily
   analyzed to determine the red-green-blue (RGB) value using portable
   devices such as desktop digital scanner and mobile phone camera,
   quantitatively. Interestingly, an orange to dark orange color transition
   was observed when the aqueous and solid paper colorimetric sensor was
   introduced to Sr2+ ion, respectively. It was demonstrated that the value
   of the signal has a linear relationship with concentrations of the
   strontium in the 500 ppm to 100 ppb range with a detection limit of 200
   ppb. We believe that a newly developed Chemo-Paper-Sensor will be useful
   in a wide range of sensing applications. (C) 2016 Elsevier Ltd. All
   rights reserved.}},
