Book-Group-Author = {{IEEE}},
Title = {{Self-Secured Control with Anomaly Detection and Recovery in Automotive
   Cyber-Physical Systems}},
Booktitle = {{2019 DESIGN, AUTOMATION \& TEST IN EUROPE CONFERENCE \& EXHIBITION
   (DATE)}},
Series = {{Design Automation and Test in Europe Conference and Exhibition}},
