@inproceedings{ ISI:000386979000057,
Author = {Zhang, Jiexin and Gan, Shaoduo and Liu, Xiaoxue and Zhu, Peidong},
Book-Group-Author = {{IEEE}},
Title = {{Intrusion Detection in SCADA Systems by Traffic Periodicity and
   Telemetry Analysis}},
