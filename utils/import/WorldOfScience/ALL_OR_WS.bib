@inproceedings{ ISI:000527802800006,
Author = {Lin, Chih-Yuan and Nadjm-Tehrani, Simin},
Book-Group-Author = {{USENIX Assoc}},
Title = {{Timing Patterns and Correlations in Spontaneous SCADA Traffic for
   Anomaly Detection}},
Booktitle = {{PROCEEDINGS OF THE 22ND INTERNATIONAL SYMPOSIUM ON RESEARCH IN ATTACKS,
   INTRUSIONS AND DEFENSES}},
Year = {{2019}},
Pages = {{73-88}},
Note = {{22nd International Symposium on Research in Attacks, Intrusions and
   Defenses (RAID), Beijing, PEOPLES R CHINA, SEP 23-25, 2019}},
Organization = {{Qi An Xin Grp; Baidu Secur; Dawning Informa Ind Co Ltd; Kryptowire;
   Topsec Network Techno Inc; Inspur Power Commercial Syst Co Ltd; Nsfocus
   Informat Technol Co Ltd; Chinese Acad Sci, Inst Software; Chinese Acad
   Sci, Inst Informat Engn}},
Abstract = {{Supervisory Control and Data Acquisition (SCADA) systems operate
   critical infrastructures in our modern society despite their
   vulnerability to attacks and misuse. There are several anomaly detection
   systems based on the cycles of polling mechanisms used in SCADA systems,
   but the feasibility of anomaly detection systems based on non-polling
   traffic, so called spontaneous events, is not well-studied. This paper
   presents a novel approach to modeling the timing characteristics of
   spontaneous events in an IEC-60870-5-104 network and exploits the model
   for anomaly detection. The system is tested with a dataset from a real
   power utility with injected timing effects from two attack scenarios.
   One attack causes timing anomalies due to persistent malfunctioning in
   the field devices, and the other generates intermittent anomalies caused
   by malware on the field devices, which is considered as stealthy. The
   detection accuracy and timing performance are promising for all the
   experiments with persistent anomalies. With intermittent anomalies, we
   found that our approach is effective for anomalies in low-volume traffic
   or attacks lasting over 1 hour.}},
