@article{ ISI:000082079400005,
Author = {Paulsen, P and Dabbass, A and Schopf, E},
Title = {{Detection of Salmonella sp on poultry meat - By means of a
   semi-automated immunoconcentration/enzyme-linked-fluorescent-assay
   system (Vidas ICS/SLM)}},
Journal = {{FLEISCHWIRTSCHAFT}},
Year = {{1999}},
Volume = {{79}},
Number = {{8}},
Pages = {{78-80}},
Abstract = {{The Vidas ICS/SLM system allows the detection of Salmonella sp. as
   antigens in food in a 4-step procedure: cultural enrichment,
   immunconcentration (TCS), post enrichment, antigen detection (SLM). A
   preliminary result can be obtained at the end of the day following the
   day of arrival of the sample, i.e. in an investigation time of 24 to 28
   hrs. This method was compared to the widely accepted rapid, sensitive
   and low-cost MSRV method. Both methods were applied on frozen as well as
   chilled poultry meat samples. After some modification of the Vidas
   ICS/SLM enrichment protocol, no significant differences between MSRV and
   Vidas SLM were found in 36 chilled and 58 frozen meat samples. The Vidas
   ICS/SLM produced no False positive results.}},
