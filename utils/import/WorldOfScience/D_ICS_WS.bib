@inproceedings{ ISI:000331072100060,
Author = {Liu, Yu and Jin, Yier and Makris, Yiorgos},
Book-Group-Author = {{IEEE}},
Title = {{Hardware Trojans in Wireless Cryptographic ICs: Silicon Demonstration \&
   Detection Method Evaluation}},
Booktitle = {{2013 IEEE/ACM INTERNATIONAL CONFERENCE ON COMPUTER-AIDED DESIGN (ICCAD)}},
Series = {{ICCAD-IEEE ACM International Conference on Computer-Aided Design}},
Year = {{2013}},
Pages = {{399-404}},
Note = {{32nd IEEE/ACM International Conference on Computer-Aided Design (ICCAD),
   San Jose, CA, NOV 18-21, 2013}},
Organization = {{IEEE; Assoc Comp Machinery; IEEE Council Elect Design Automat; Assoc
   Comp Machinery Special Interest Grp Design Automat; Electron Devices
   Soc; SSCS}},
Abstract = {{We present a silicon implementation of a hardware Trojan, which is
   capable of leaking the secret key of a wireless cryptographic integrated
   circuit (IC) consisting of an Advanced Encryption Standard (AES) core
   and an Ultra-Wide-Band (UWB) transmitter. With its impact carefully
   hidden in the transmission specification margins allowed for process
   variations, this hardware Trojan cannot be detected by production
   testing methods of either the digital or the analog part of the IC and
   does not violate the transmission protocol or any system-level
   specifications. Nevertheless, the informed adversary, who knows what to
   look for in the transmission power waveform, is capable of retrieving
   the 128-bit AES key, which is leaked with every 128-bit ciphertext block
   sent by the UWB transmitter. Using silicon measurements from 40 chips
   fabricated in TSMC's 0.35 mu m technology, we also assess the
   effectiveness of a side channel-based statistical analysis method in
   detecting this hardware Trojan.}},
