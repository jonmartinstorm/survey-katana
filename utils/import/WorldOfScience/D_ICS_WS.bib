@inproceedings{ ISI:000403604400328,
Author = {Shleymovich, M. P. and Medvedev, M. V. and Lyasheva, S. A.},
Book-Group-Author = {{IEEE}},
Title = {{Object Detection in the Images in Industrial Process Control Systems
   based on Salient Points of Wavelet Transform Analysis}},
Booktitle = {{2016 2ND INTERNATIONAL CONFERENCE ON INDUSTRIAL ENGINEERING,
   APPLICATIONS AND MANUFACTURING (ICIEAM)}},
Year = {{2016}},
Note = {{2nd International Conference on Industrial Engineering, Applications and
   Manufacturing (ICIEAM), Chelyabinsk, RUSSIA, MAY 19-20, 2016}},
Organization = {{Russia Siberia Sect Joint PES IES IAS CSS Chapter; S Ural State Univ;
   IEEE}},
Abstract = {{The article considers the task of object detection on the images in
   industrial process control systems. The task is solved in two stages:
   image segmentation and matching the images of the segments with the
   template values. A new method based on wavelet transform salient points
   detection is proposed for image segmentation. Salient point descriptors
   are used for image matching; the new method of descriptor construction
   based on wavelet transform is proposed. The unified mathematical tool of
   the wavelet transform used for image segmentation and image matching
   decreases computational load for objects enabling detection tasks
