Series = {{International Conference on Synthesis Modeling Analysis and Simulation
   Methods and Applications to Circuit Design}},
Year = {{2017}},
Note = {{13th Conference on Ph.D. Research in Microelectronics and Electronics
   (PRIME) / 14th International Conference on Synthesis, Modeling, Analysis
   and Simulation Methods and Application to Circuit Design (SMACD), ITALY,
   JUN 12-15, 2017}},
Organization = {{Micron Fdn; ST Microelectron; Dialog Semicond; ON Semicond; Cadence Acad
   Network; Coilcraft; Texas Instruments; InvenSense; River Publishers;
