@inproceedings{ ISI:000469326500037,
Author = {Liu, Junjiao and Yin, Libo and Hu, Yan and Lv, Shichao and Sun, Limin},
Book-Group-Author = {{IEEE}},
Title = {{A Novel Intrusion Detection Algorithm for Industrial Control Systems
   Based on CNN and Process State Transition}},
