@inproceedings{ ISI:000516620100042,
Author = {Lin, Qin and Adepu, Sridhar and Verwer, Sicco and Mathur, Aditya},
Book-Group-Author = {{ACM}},
Title = {{TABOR: A Graphical Model-based Approach for Anomaly Detection in
   Industrial Control Systems}},
Booktitle = {{PROCEEDINGS OF THE 2018 ACM ASIA CONFERENCE ON COMPUTER AND
   COMMUNICATIONS SECURITY (ASIACCS'18)}},
Year = {{2018}},
Pages = {{525-536}},
