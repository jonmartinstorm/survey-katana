@inproceedings{ ISI:000406427200012,
Author = {Lin, Yufeng and Ray, Biplob and Jarvis, Dennis and Wang, Jia},
Editor = {{Batten, L and Li, G}},
Title = {{False Signal Injection Attack Detection of Cyber Physical Systems by
   Event-Triggered Distributed Filtering over Sensor Networks}},
Booktitle = {{APPLICATIONS AND TECHNIQUES IN INFORMATION SECURITY}},
Series = {{Communications in Computer and Information Science}},
