@article{ ISI:000374969400003,
Author = {Dong, Mianxiong and Ota, Kaoru and Yang, Laurence T. and Liu, Anfeng and
   Guo, Minyi},
Title = {{LSCD: A Low-Storage Clone Detection Protocol for Cyber-Physical Systems}},
